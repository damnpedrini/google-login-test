{
  "version": 2,
  "builds": [
    {
      "src": "server/api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/save-credentials",
      "dest": "/server/api/save-credentials.js"
    },
    {
      "src": "/api/save-email-sheets",
      "dest": "/server/api/save-email-sheets.js"
    },
    {
      "src": "/api/status",
      "dest": "/server/api/status.js"
    },
    {
      "src": "/",
      "dest": "/server/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server/api/index.js"
    }
  ]
}
