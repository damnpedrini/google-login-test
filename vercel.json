{
  "version": 2,
  "builds": [
    {
      "src": "server/api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/save-credentials",
      "dest": "/server/api/save-credentials.js"
    },
    {
      "src": "/api/download-csv",
      "dest": "/server/api/download-csv.js"
    },
    {
      "src": "/api/get-credentials",
      "dest": "/server/api/get-credentials.js"
    },
    {
      "src": "/api/status",
      "dest": "/server/api/status.js"
    },
    {
      "src": "/",
      "dest": "/server/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server/api/index.js"
    }
  ]
}
